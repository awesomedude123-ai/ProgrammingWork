package com.sri.course;

public class Inheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Inheritance
		//Interfaces
		//Data Structures-> Collections -> Drawers, -> Cupbaords -> Cabinets
		//Array[], ArrayLists(), Lists()
		
		String[] name = new String[9];
		
		/**
		 * String methods->
		 * String name = "This is the coolest";
		name.equals("This is cool");
		name.equalsIgnoreCase("This is cool");
		name.length();
		name.concat("This is coolest");
		name.substring(4);
		 */
		
		
	}//end main method

}//end inheritance
