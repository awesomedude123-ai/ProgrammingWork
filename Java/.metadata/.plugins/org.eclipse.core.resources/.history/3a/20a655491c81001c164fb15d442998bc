package Tsa2022;
import java.util.*;

public class PartC_2152_901 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner oldScanner = new Scanner(System.in);
		for(int i=0;i<3;i++){//Java
			String[][] a1 = new String[4][2];//J
			String[][] a2 = new String[4][2];//A
			String[][] a3 = new String[2][4];//V
			String[][] a4 = new String[2][4];//A
			String word = oldScanner.nextLine();
			for(int x=0;x<word.length();x++) {
				int y=(int)word.charAt(x);
				String b = Integer.toBinaryString(y);
				boolean l = true;
				String bin = "";
				
				while (l == true) {
					
					
					if (8 - b.length() == bin.length()) {
						l = false;
					} else {
						bin = bin + "0";
					} // end 1 if/else loop
				} // end while loop
				bin = bin + b;
				int index = 7;
				if(x==0) {
					for(int y1=0;y1<4;y1++) {
						for(int x1=1;x1>=0;x1--) {
							if(bin.charAt(index)=='0') {
								a1[y1][x1]="  ";
								System.out.print(a1[y1][x1]);
							}else {
								a1[y1][x1]="# ";
								System.out.print(a1[y1][x1]);
							}
							index--;
							
						}
						System.out.println();
					}
				}else if(x==1) {
					System.out.println("here");
					for(int y1=0;y1<4;y1++) {
						for(int x1=1;x1>=0;x1--) {
							if(bin.charAt(index)=='0') {
								a2[y1][x1]="  ";
								System.out.print(a2[y1][x1]);
							}else {
								a2[y1][x1]="# ";
								System.out.print(a2[y1][x1]);
							}
							index--;
							
						}
						System.out.println();
					}
				}else if(x==2) {
					System.out.println("This is it");
					for(int y1=0;y1<=1;y1++) {
						for(int x1=0;x1<4;x1++) {
							if(bin.charAt(index)=='0') {
								a3[y1][x1]="  ";
								System.out.print(a3[y1][x1]);
							}else {
								a3[y1][x1]="# ";
								System.out.print(a3[y1][x1]);
							}
							index--;
						}
						System.out.println();
					}
				}else {
					System.out.println("This is it");
					for(int y1=0;y1<=1;y1++) {
						for(int x1=0;x1<4;x1++) {
							if(bin.charAt(index)=='0') {
								a4[y1][x1]="  ";
								System.out.print(a4[y1][x1]);
							}else {
								a3[y1][x1]="# ";
								System.out.print(a4[y1][x1]);
							}
							index--;
						}
						System.out.println();
					}
				}
				
			}
		}
		oldScanner.close();
	}//end main method

}//end public class
