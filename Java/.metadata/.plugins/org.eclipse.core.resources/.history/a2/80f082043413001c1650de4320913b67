package com.sri.course;

public class Inheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Inheritance
		//Interfaces
		//Data Structures-> 
		//String methods->
		
		String name = "This is the coolest";
		name.equals("This is cool");
		name.equalsIgnoreCase("This is cool");

	}//end main method

}//end inheritance
